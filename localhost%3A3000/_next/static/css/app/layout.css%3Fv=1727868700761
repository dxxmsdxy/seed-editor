@keyframes justSet {
	from
	{
		background-color:#fff4;
		color:#000b
	}

	12%
	{
		background-color:#fff8;
		color:#000b
	}

	25%
	{
		background-color:#000d;
		color:#fffb
	}

	45%
	{
		background-color:#000b;
		color:#000b
	}

	65%
	{
		background-color:#000e;
		color:#fffb
	}

	80%
	{
		background-color:#000c;
		color:#000b
