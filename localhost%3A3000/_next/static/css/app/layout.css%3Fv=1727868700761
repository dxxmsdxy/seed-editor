@keyframes colorCycle {
  from {
    filter: contrast(95%) saturate(95%) hue-rotate(0deg);
  }
  to {
    filter: contrast(95%) saturate(95%) hue-rotate(360deg);
    stroke-dashoffset: 1000;
  }
}
