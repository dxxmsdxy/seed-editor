  from {
    filter: contrast(95%) saturate(100%) hue-rotate(0deg);
  }
  50% {
    filter: contrast(95%) saturate(95%) hue-rotate(80deg);
  }
  to {
    filter: contrast(95%) saturate(100%) hue-rotate(0deg);
  }
}
@keyframes pulseCycle {
  from {
    filter: blur(0.3px) contrast(108%) saturate(108%);
  }
  50% {
    filter: blur(0.3px) contrast(96%) saturate(96%);
  }
  to {
    filter: blur(0.3px) contrast(108%) saturate(108%);
  }
}

